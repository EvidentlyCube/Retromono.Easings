{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "src": "../"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
	  "properties": {
		"TargetFramework": "netstandard2.0"
	  },
	  "_disableNavbar": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
		  "favicon.ico",
		  "logo.svg"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "bin/**"
        ]
      }
    ],
    "dest": "bin",
	"globalMetadata": {
		"_appTitle": "Evidently Cube",
		"_enableSearch": true,
		"_disableNavbar": false,
		"_disableBreadcrumb": false,
		"_appFaviconPath": "favicon.ico",
		"_appFooter": "<span>Copyright Â© 2018 <a href='https://www.evidentlycube.com'>Evidently Cube</a><br>Generated by <strong>DocFX</strong></span>"
	},
    "template": [
      "statictoc",
	  "evidently_template"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}